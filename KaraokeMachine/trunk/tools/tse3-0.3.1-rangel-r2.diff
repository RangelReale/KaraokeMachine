+
+PlayableIterator *TextTrack::iterator(Clock index)
+{
+    return new TextTrackIterator(this, index);
+}
+
+
+Clock TextTrack::lastClock() const
+{
+    return (!data.empty()) ? data[size()-1].time : Clock(0);
+}
